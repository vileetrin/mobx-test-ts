{
  "printWidth": 80,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "max-line-length": [
    true,
    {
      "limit": 140,
      "ignore-pattern": "^import |^export {(.*?)}"
    }
  ]
}